#include <stdio.h>

int main(void) {
int D,a,b,c,d,i,t,total;
scanf("%d",&t);
for(i=0;i<t;i++)
{
scanf("%d %d %d %d %d",&D,&d,&a,&b,&c);
total=d*D;
if(total<10)
printf("0\n");
else if(total>=10 && total<21)
printf("%d\n",a);
else if(total>=21 && total<42)
printf("%d\n",b);
else
printf("%d\n",c);
}
return 0;
}
